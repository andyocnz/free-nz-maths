{
  "years": [
    {
      "year": "Olympiad",
      "skills": [
        {
          "id": "Y13.NT.DIVISIBILITY_SQUARE_POWER",
          "strand": "Number Theory",
          "name": "Divisibility and Perfect Powers",
          "description": "When is n^n a perfect square?",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.NT.DIVISIBILITY_SQUARE_POWER.T1",
              "stem": "For how many positive integers n between 1 and {limit} inclusive is n^n a perfect square?",
              "params": {
                "limit": ["int", 500, 2000, 100]
              },
              "answer": "{Math.floor(limit / 2) + Math.floor(Math.sqrt(limit)) - Math.floor(Math.sqrt(limit) / 2)}",
              "difficulty": 10
            }
          ]
        },
        {
          "id": "Y13.NT.LCM_HCF_PRODUCT",
          "strand": "Number Theory",
          "name": "HCF and LCM Relations",
          "description": "Relationship between HCF and LCM of two integers",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.NT.LCM_HCF_PRODUCT.T1",
              "stem": "The LCM of two positive integers is {lcm} and their HCF is {hcf}. If one of the numbers is {a}, what is the other number?",
              "params": {
                "hcf": ["int", 12, 48, 6],
                "f1": ["int", 3, 10],
                "f2": ["int", 5, 15],
                "a": "{hcf * f1}",
                "lcm": "{hcf * f1 * f2}"
              },
              "answer": "{hcf * f2}",
              "difficulty": 9
            }
          ]
        },
        {
          "id": "Y13.NT.MODULAR_BINOMIAL",
          "strand": "Number Theory",
          "name": "Modular Arithmetic with Binomials",
          "description": "(d-1)^n + (d+1)^n modulo d^2",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.NT.MODULAR_BINOMIAL.T1",
              "stem": "Let a_n = ({base}-1)^n + ({base}+1)^n. Find a_{exp} modulo {mod}.",
              "params": {
                "base": ["choice", 7, 10, 13],
                "mod": "{base * base}",
                "exp": ["int", 50, 150]
              },
              "answer": "{(powMod(base - 1, exp, mod) + powMod(base + 1, exp, mod)) % mod}",
              "difficulty": 10
            }
          ]
        },
        {
          "id": "Y13.NT.BASE_CONVERSION",
          "strand": "Number Theory",
          "name": "Base Conversion",
          "description": "Reverse base conversion problems",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.NT.BASE_CONVERSION.T1",
              "stem": "The number {value} in base 10 is written as '{digits}' in base b (where each digit is less than b). Find b.",
              "params": {
                "b": ["int", 11, 20],
                "d2": ["int", 1, "{b-1}"],
                "d1": ["int", 0, "{b-1}"],
                "d0": ["int", 1, "{b-1}"],
                "digits": "{d2}{d1}{d0}",
                "value": "{d2*b*b + d1*b + d0}"
              },
              "answer": "{b}",
              "difficulty": 9
            }
          ]
        },
        {
          "id": "Y13.NT.SUM_DIVISORS",
          "strand": "Number Theory",
          "name": "Sum of Divisors",
          "description": "Sum of divisors function",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.NT.SUM_DIVISORS.T1",
              "stem": "Find the sum of all positive divisors of {n}.",
              "params": {
                "n": ["int", 1000, 100000, 1000]
              },
              "answer": "{sumOfDivisors(n)}",
              "difficulty": 9
            }
          ]
        },
        {
          "id": "Y13.ALG.DIOPHANTINE_COUNT",
          "strand": "Algebra",
          "name": "Linear Diophantine Equations",
          "description": "Positive solutions to linear Diophantine equations",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.ALG.DIOPHANTINE_COUNT.T1",
              "stem": "How many ordered pairs of positive integers (x, y) satisfy {a}x + {b}y = {target}?",
              "params": {
                "a": ["int", 3, 15],
                "b": ["int", 4, 16],
                "target": ["int", 200, 1000, 50]
              },
              "answer": "{countPositiveDiophantine(a, b, target)}",
              "difficulty": 10
            }
          ]
        },
        {
          "id": "Y13.ALG.TAN_RECURRENCE",
          "strand": "Algebra",
          "name": "Tangent Recurrence",
          "description": "Tangent recurrence relation (AIME 2001)",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.ALG.TAN_RECURRENCE.T1",
              "stem": "The sequence is defined by x_0 = {num}/{den} and x_{n+1} = ({den}x_n + {num}) / ({den} - {num}x_n). Find x_{term}.",
              "params": {
                "den": ["int", 5, 12],
                "num": ["int", 1, "{den-1}"],
                "term": ["int", 100, 500, 50]
              },
              "answer": "{tanRecurrenceValue(den, num, term)}",
              "difficulty": 10
            }
          ]
        },
        {
          "id": "Y13.ALG.SCALING_FUNCTION",
          "strand": "Algebra",
          "name": "Homogeneous Functional Equations",
          "description": "Scaling and homogeneous functional equations",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.ALG.SCALING_FUNCTION.T1",
              "stem": "A function satisfies f({k}x) = {k}f(x) for all x > 0 and f(x) = 1 - |x-2| for 1 <= x <= 3. Find f({target}).",
              "params": {
                "k": ["choice", 3, 4, 5],
                "target": ["int", 500, 5000, 100]
              },
              "answer": "{scalingFunctionValue(k, target)}",
              "difficulty": 10
            }
          ]
        },
        {
          "id": "Y13.ALG.PRIME_ROOTS_QUADRATIC",
          "strand": "Algebra",
          "name": "Quadratic with Prime Roots",
          "description": "Quadratic equation with two prime roots",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.ALG.PRIME_ROOTS_QUADRATIC.T1",
              "stem": "The roots of x^2 - {sum}x + k = 0 are both prime numbers. Find the value of k.",
              "params": {
                "p2": ["int", 50, 200],
                "sum": "{2 + p2}"
              },
              "answer": "{2 * p2}",
              "difficulty": 8
            }
          ]
        },
        {
          "id": "Y13.ALG.FUNCTIONAL_INVERSE",
          "strand": "Algebra",
          "name": "Functional Equations with Inverse",
          "description": "Functional equation with f(1/x) term",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.ALG.FUNCTIONAL_INVERSE.T1",
              "stem": "f(x) + {c}f(1/x) = {d}x for all x != 0. Find f({val}).",
              "params": {
                "c": ["int", 2, 6],
                "d": ["int", 2, 8],
                "val": ["int", 2, 10]
              },
              "answer": "{solveInverseFunctional(c, d, val)}",
              "difficulty": 10
            }
          ]
        },
        {
          "id": "Y13.ALG.TELESCOPING_HARMONIC",
          "strand": "Algebra",
          "name": "Telescoping Series",
          "description": "Telescoping partial fractions",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.ALG.TELESCOPING_HARMONIC.T1",
              "stem": "Find the exact value of 1/(1*2) + 1/(2*3) + ... + 1/({n}*({n}+1)).",
              "params": {
                "n": ["int", 100, 1000, 50]
              },
              "answer": "{n / (n + 1)}",
              "difficulty": 8
            }
          ]
        },
        {
          "id": "Y13.ALG.POLY_REMAINDER_LINEAR",
          "strand": "Algebra",
          "name": "Polynomial Remainder Theorem",
          "description": "Polynomial remainder with linear factors",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.ALG.POLY_REMAINDER_LINEAR.T1",
              "stem": "When P(x) is divided by (x - {a}) the remainder is {ra}, and by (x - {b}) the remainder is {rb}. When divided by (x - {a})(x - {b}) the remainder is cx + d. Find c + d.",
              "params": {
                "a": ["int", 1, 5],
                "b": ["int", 6, 10],
                "ra": ["int", -20, 20],
                "rb": ["int", -20, 20]
              },
              "answer": "{polyLinearRemainderSum(a, b, ra, rb)}",
              "difficulty": 10
            }
          ]
        },
        {
          "id": "Y13.GEOM.CUBE_ANT",
          "strand": "Geometry",
          "name": "Shortest Path on Cube",
          "description": "Ant moving on cube surface",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.GEOM.CUBE_ANT.T1",
              "stem": "An ant moves along the surface of a cube with side length {s} from one vertex to the opposite vertex. What is the shortest possible distance?",
              "params": {
                "s": ["int", 2, 20]
              },
              "answer": "{Math.sqrt(5) * s}",
              "difficulty": 9
            }
          ]
        },
        {
          "id": "Y13.GEOM.INRADIUS_RIGHT_TRIANGLE",
          "strand": "Geometry",
          "name": "Inradius of Pythagorean Triples",
          "description": "Inscribed circle of right triangle",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.GEOM.INRADIUS_RIGHT_TRIANGLE.T1",
              "stem": "A right triangle has legs {a} and {b} and hypotenuse {c}. Find the radius of its inscribed circle.",
              "params": {
                "m": ["int", 3, 10],
                "n": ["int", 1, "{m-1}"],
                "a": "{m*m - n*n}",
                "b": "{2*m*n}",
                "c": "{m*m + n*n}"
              },
              "answer": "{(a + b - c) / 2}",
              "difficulty": 9
            }
          ]
        },
        {
          "id": "Y13.GEOM.COORDINATE_AREA",
          "strand": "Geometry",
          "name": "Coordinate Geometry - Area",
          "description": "Shoelace formula for triangle area",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.GEOM.COORDINATE_AREA.T1",
              "stem": "Find the area of the triangle with vertices ({x1},{y1}), ({x2},{y2}), ({x3},{y3}).",
              "params": {
                "x1": ["int", -10, 10],
                "y1": ["int", -10, 10],
                "x2": ["int", -10, 10],
                "y2": ["int", -10, 10],
                "x3": ["int", -10, 10],
                "y3": ["int", -10, 10]
              },
              "answer": "{Math.abs(x1*(y2-y3) + x2*(y3-y1) + x3*(y1-y2)) / 2}",
              "difficulty": 8
            }
          ]
        },
        {
          "id": "Y13.PROB.BUG_TRIANGLE",
          "strand": "Probability",
          "name": "Random Walk on Triangle",
          "description": "Probability of returning to start",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.PROB.BUG_TRIANGLE.T1",
              "stem": "A bug starts at a vertex of a triangle and at each step moves to one of the two adjacent vertices with equal probability. After {steps} moves, what is the probability it is back at the start?",
              "params": {
                "steps": ["int", 10, 30, 2]
              },
              "answer": "{triangleRandomWalkProbability(steps)}",
              "difficulty": 9
            }
          ]
        },
        {
          "id": "Y13.PROB.EXPECTED_WOMEN",
          "strand": "Probability",
          "name": "Linearity of Expectation",
          "description": "Expected value in committee selection",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.PROB.EXPECTED_WOMEN.T1",
              "stem": "A committee of {size} people is chosen at random from {men} men and {women} women. What is the expected number of women on the committee?",
              "params": {
                "men": ["int", 5, 15],
                "women": ["int", 8, 20],
                "size": ["int", 4, 10]
              },
              "answer": "{size * women / (men + women)}",
              "difficulty": 8
            }
          ]
        },
        {
          "id": "Y13.COMB.DERANGEMENT",
          "strand": "Combinatorics",
          "name": "Derangements",
          "description": "No fixed points permutations",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.COMB.DERANGEMENT.T1",
              "stem": "In how many ways can {n} distinct letters be placed into {n} envelopes so that no letter goes into its correct envelope?",
              "params": {
                "n": ["int", 7, 12]
              },
              "answer": "{derangement(n)}",
              "difficulty": 9
            }
          ]
        },
        {
          "id": "Y13.COMB.STARS_BARS",
          "strand": "Combinatorics",
          "name": "Stars and Bars",
          "description": "Non-negative integer solutions",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.COMB.STARS_BARS.T1",
              "stem": "Find the number of non-negative integer solutions to x_1 + x_2 + ... + x_{k} = {n}.",
              "params": {
                "n": ["int", 20, 100, 10],
                "k": ["int", 3, 8]
              },
              "answer": "{binomial_coeff(n + k - 1, k - 1)}",
              "difficulty": 9
            }
          ]
        },
        {
          "id": "Y13.COMB.PIGEONHOLE_SUM",
          "strand": "Combinatorics",
          "name": "Pigeonhole Principle",
          "description": "Pigeonhole principle applications",
          "phase": 13,
          "templates": [
            {
              "id": "Y13.COMB.PIGEONHOLE_SUM.T1",
              "stem": "What is the smallest number of integers that must be chosen from 1..{m} to guarantee that at least two sum to {m}+1?",
              "params": {
                "m": ["int", 10, 30, 2]
              },
              "answer": "{Math.floor(m / 2) + 1}",
              "difficulty": 7
            }
          ]
        }
      ]
    }
  ]
}
