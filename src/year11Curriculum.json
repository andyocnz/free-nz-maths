{
  "years": [
    {
      "year": 11,
      "skills": [
        {
          "id": "Y11.A.POLYNOMIALS",
          "strand": "Number & Algebra",
          "name": "Polynomial operations",
          "description": "Expand, factor, and simplify polynomials",
          "phase": 10,
          "templates": [
            {
              "id": "Y11.A.POLYNOMIALS.T1",
              "stem": "Expand ({a}x + {b})({c}x + {d})",
              "params": { "a": ["int", 1, 5], "b": ["int", -10, 10], "c": ["int", 1, 5], "d": ["int", -10, 10] },
              "answer": "{a*c}x^2 + {a*d + b*c}x + {b*d}",
              "difficulty": 6
            },
            {
              "id": "Y11.A.POLYNOMIALS.T3",
              "stem": "Simplify: ({a}x^2 + {b}x + {c}) + ({d}x^2 + {e}x + {f})",
              "params": { "a": ["int", 1, 5], "b": ["int", -10, 10], "c": ["int", -10, 10], "d": ["int", 1, 5], "e": ["int", -10, 10], "f": ["int", -10, 10] },
              "answer": "{a+d}x^2 + {b+e}x + {c+f}",
              "difficulty": 5
            }
          ]
        },
        {
          "id": "Y11.G.TRIG_RATIOS",
          "strand": "Geometry & Measurement",
          "name": "Trigonometric ratios",
          "description": "Calculate sine, cosine, tangent in right triangles",
          "phase": 10,
          "templates": [
            {
              "id": "Y11.G.TRIG_RATIOS.T1",
              "stem": "In a right triangle, opposite = {a}, adjacent = {b}. Find tan(θ).",
              "params": { "a": ["int", 3, 20], "b": ["int", 3, 20] },
              "answer": "{a/b}",
              "difficulty": 6
            },
            {
              "id": "Y11.G.TRIG_RATIOS.T3",
              "stem": "Hypotenuse = {c}, opposite = {a}. Find sin(θ).",
              "params": { "c": ["int", 10, 25], "a": ["int", 5, 20] },
              "answer": "{a/c}",
              "difficulty": 6
            }
          ]
        },
        {
          "id": "Y11.G.COORDINATE_GEOMETRY",
          "strand": "Geometry & Measurement",
          "name": "Coordinate geometry",
          "description": "Lines, gradients, distances, midpoints",
          "phase": 10,
          "templates": [
            {
              "id": "Y11.G.COORDINATE_GEOMETRY.T1",
              "stem": "Find the gradient of the line through ({x1},{y1}) and ({x2},{y2}).",
              "params": { "x1": ["int", -10, 10], "y1": ["int", -10, 10], "x2": ["int", -10, 10], "y2": ["int", -10, 10] },
              "answer": "(y2-y1)/(x2-x1)",
              "difficulty": 6
            },
            {
              "id": "Y11.G.COORDINATE_GEOMETRY.T2",
              "stem": "Find the distance between ({x1},{y1}) and ({x2},{y2}).",
              "params": { "x1": ["int", -10, 10], "y1": ["int", -10, 10], "x2": ["int", -10, 10], "y2": ["int", -10, 10] },
              "answer": "sqrt((x2-x1)*(x2-x1) + (y2-y1)*(y2-y1))",
              "difficulty": 7
            },
            {
              "id": "Y11.G.COORDINATE_GEOMETRY.T3",
              "stem": "Find the midpoint of ({x1},{y1}) and ({x2},{y2}).",
              "params": { "x1": ["int", -10, 10], "y1": ["int", -10, 10], "x2": ["int", -10, 10], "y2": ["int", -10, 10] },
              "answer": "({(x1+x2)/2}, {(y1+y2)/2})",
              "difficulty": 5
            },
            {
              "id": "Y11.G.COORDINATE_GEOMETRY.T4",
              "stem": "Write the equation of the line with gradient {m} through ({x1},{y1}).",
              "params": { "m": ["int", -5, 5], "x1": ["int", -10, 10], "y1": ["int", -10, 10] },
              "answer": "y = {m}x + {y1 - m*x1}",
              "difficulty": 7
            }
          ]
        },
        {
          "id": "Y11.S.DESCRIPTIVE_STATS",
          "strand": "Statistics & Probability",
          "name": "Descriptive statistics",
          "description": "Mean, median, mode, range, standard deviation",
          "phase": 10,
          "templates": [
            {
              "id": "Y11.S.DESCRIPTIVE_STATS.T1",
              "stem": "Find the mean of: {a}, {b}, {c}, {d}, {e}.",
              "params": { "a": ["int", 10, 100], "b": ["int", 10, 100], "c": ["int", 10, 100], "d": ["int", 10, 100], "e": ["int", 10, 100] },
              "answer": "(a + b + c + d + e) / 5",
              "difficulty": 5
            },
            {
              "id": "Y11.S.DESCRIPTIVE_STATS.T3",
              "stem": "Calculate the range of: {a}, {b}, {c}, {d}, {e}.",
              "params": { "a": ["int", 10, 100], "b": ["int", 10, 100], "c": ["int", 10, 100], "d": ["int", 10, 100], "e": ["int", 10, 100] },
              "answer": "max([a, b, c, d, e]) - min([a, b, c, d, e])",
              "difficulty": 4
            }
          ]
        },
        {
          "id": "Y11.C.CALCULUS_DIFF",
          "strand": "Calculus",
          "name": "Differentiation",
          "description": "Differentiate polynomials and interpret gradients.",
          "phase": 10,
          "templates": [
            {
              "id": "Y11.C.CALCULUS_DIFF.T1",
              "stem": "Differentiate f(x) = {a}x^2 + {b}x + {c}.",
              "params": { "a": ["int", 1, 5], "b": ["int", -10, 10], "c": ["int", -5, 5] },
              "answer": "f'(x) = {2*a}x + {b}",
              "difficulty": 10
            },
            {
              "id": "Y11.C.CALCULUS_DIFF.T2",
              "stem": "Find dy/dx for y = {a}x^3 + {b}x^2.",
              "params": { "a": ["int", 1, 5], "b": ["int", -10, 10] },
              "answer": "dy/dx = {3*a}x^2 + {2*b}x",
              "difficulty": 10
            },
            {
              "id": "Y11.C.CALCULUS_DIFF.T3",
              "stem": "Find the gradient of y = {a}x^2 at x = {x0}.",
              "params": { "a": ["int", 1, 5], "x0": ["int", -5, 5] },
              "answer": "2*a*x0",
              "difficulty": 9
            }
          ]
        }
      ]
    }
  ]
}
