        {
          "id": "OLYMP.NT.DIVISIBILITY_SQUARE_POWER",
          "strand": "Number Theory",
          "name": "When is nⁿ a perfect square?",
          "description": "An Olympiad-level question in the strand of Number Theory about When is nⁿ a perfect square?.",
          "templates": [
            {
              "id": "OLYMP.NT.DIVISIBILITY_SQUARE_POWER.T1",
              "stem": "For how many positive integers n between 1 and {limit} inclusive is n^n a perfect square?",
              "params": {
                "limit": [
                  "int",
                  500,
                  2000,
                  100
                ]
              },
              "answer": "{Math.floor(limit / 2) + Math.floor(Math.sqrt(limit)) - Math.floor(Math.sqrt(limit) / 2)}",
              "difficulty": 13,
              "phase": 13
            }
          ]
        },
        {
          "id": "OLYMP.NT.LCM_HCF_PRODUCT",
          "strand": "Number Theory",
          "name": "HCF and LCM relation",
          "description": "An Olympiad-level question in the strand of Number Theory about HCF and LCM relation.",
          "templates": [
            {
              "id": "OLYMP.NT.LCM_HCF_PRODUCT.T1",
              "stem": "The LCM of two positive integers is {lcm} and their HCF is {hcf_value}. If one of the numbers is {num1}, what is the other number?",
              "params": {
                "hcf_value": [ "int", 2, 50 ],
                "f1": [ "int", 1, 40 ],
                "f2": [ "int", 1, 40 ],
                "num1": "{hcf_value * f1}",
                "num2": "{hcf_value * f2}",
                "lcm": "{hcf_value * f1 * f2}"
              },
              "validateParams": "f1 != f2 && hcf(f1, f2) == 1",
              "answer": "{num2}",
              "difficulty": 13,
              "phase": 13
            }
          ]
        },
        {
          "id": "OLYMP.NT.MODULAR_BINOMIAL",
          "strand": "Number Theory",
          "name": "Lifting The Exponent Lemma",
          "description": "Evaluating (p−1)^n + (p+1)^n modulo p² and related LTE applications.",
          "templates": [
            {
              "id": "OLYMP.NT.MODULAR_BINOMIAL.T1",
              "stem": "Let a_n = ({base}-1)^n + ({base}+1)^n. Find a_{exp} modulo {mod}.",
              "params": {
                "base": [
                  "choice",
                  7,
                  10,
                  13
                ],
                "mod": "{base * base}",
                "exp": [
                  "int",
                  50,
                  150
                ]
              },
              "answer": "{(powMod(base - 1, exp, mod) + powMod(base + 1, exp, mod)) % mod}",
              "difficulty": 13,
              "phase": 13
            }
          ]
        },
